{
  "workspace_id": "",
  "collection_id": "",
  "environment_id": "",
  "collection_name": "AI Health Intelligence API",
  "environment_name": "Local Development",
  "base_url": "http://localhost:8000/api",
  "endpoints": [
    {
      "name": "Health Analysis",
      "method": "POST",
      "path": "/health/analyze",
      "requires_auth": true,
      "description": "Primary health analysis endpoint with Firebase authentication"
    },
    {
      "name": "Health Assessment",
      "method": "POST",
      "path": "/assess",
      "requires_auth": false,
      "description": "Health assessment without authentication"
    },
    {
      "name": "Get User Profile",
      "method": "GET",
      "path": "/user/profile",
      "requires_auth": true,
      "description": "Retrieve authenticated user's profile"
    },
    {
      "name": "Update User Profile",
      "method": "PUT",
      "path": "/user/profile",
      "requires_auth": true,
      "description": "Update authenticated user's profile"
    },
    {
      "name": "Get User Statistics",
      "method": "GET",
      "path": "/user/statistics",
      "requires_auth": true,
      "description": "Get user assessment statistics"
    },
    {
      "name": "Get Assessment History",
      "method": "GET",
      "path": "/user/assessments",
      "requires_auth": true,
      "description": "Get paginated assessment history"
    },
    {
      "name": "Get Assessment Detail",
      "method": "GET",
      "path": "/user/assessments/{assessment_id}",
      "requires_auth": true,
      "description": "Get detailed assessment information"
    },
    {
      "name": "System Status",
      "method": "GET",
      "path": "/status",
      "requires_auth": false,
      "description": "Get system status"
    },
    {
      "name": "Health Check",
      "method": "GET",
      "path": "/health",
      "requires_auth": false,
      "description": "Simple health check"
    },
    {
      "name": "Model Info",
      "method": "GET",
      "path": "/model/info",
      "requires_auth": false,
      "description": "Get ML model information"
    },
    {
      "name": "List Diseases",
      "method": "GET",
      "path": "/diseases",
      "requires_auth": false,
      "description": "Get list of supported diseases"
    },
    {
      "name": "Top Predictions",
      "method": "POST",
      "path": "/predict/top",
      "requires_auth": false,
      "description": "Get top N disease predictions"
    }
  ]
}
