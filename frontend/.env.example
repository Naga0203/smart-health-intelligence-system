# ============================================================================
# Frontend Environment Variables Template
# ============================================================================
# Copy this file to .env and fill in your actual values
# NEVER commit the actual .env file to version control!

# ============================================================================
# API Configuration
# ============================================================================
# Backend API base URL
# Development: http://localhost:8000
# Production: https://your-api-domain.com
VITE_API_BASE_URL=http://localhost:8000

# ============================================================================
# Firebase Configuration
# ============================================================================
# Get these values from your Firebase project settings
# Firebase Console: https://console.firebase.google.com/

# Firebase API Key
VITE_FIREBASE_API_KEY=your_firebase_api_key_here

# Firebase Auth Domain (usually: your-project-id.firebaseapp.com)
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com

# Firebase Project ID
VITE_FIREBASE_PROJECT_ID=your-project-id

# Firebase Storage Bucket (usually: your-project-id.appspot.com)
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com

# Firebase Messaging Sender ID
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id

# Firebase App ID
VITE_FIREBASE_APP_ID=your_firebase_app_id

# ============================================================================
# Optional Configuration
# ============================================================================

# Google Analytics Measurement ID (optional)
# VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Sentry DSN for error tracking (optional)
# VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Feature flags (optional)
# VITE_ENABLE_ANALYTICS=true
# VITE_ENABLE_OFFLINE_MODE=true

# Medical Report Upload Feature
# Enable/disable medical report upload feature (true/false)
VITE_ENABLE_REPORT_UPLOAD=true
